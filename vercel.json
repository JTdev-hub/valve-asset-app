{
  "version": 2,
  "builds": [
    {
      "src": "src/server/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/createCustomer",
      "dest": "/"
    },
    {
      "src": "/viewCustomers",
      "dest": "/"
    },
    {
      "src": "/createAssetHeaders",
      "dest": "/"
    },
    {
      "src": "/viewAssetHeaders",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
